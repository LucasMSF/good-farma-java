<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="650.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.goodfarma.controller.MenuController">
    <Pane prefHeight="650.0" prefWidth="360.0" style="-fx-background-color: #0373fc;">
        <children>
            <ImageView fitHeight="150.0" fitWidth="350.0" layoutX="6.0" layoutY="68.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@../../img/logo.png" />
                </image>
            </ImageView>
         <Button fx:id="btnBack" layoutX="14.0" layoutY="564.0" mnemonicParsing="false" onAction="#onBtnExitClick" prefHeight="56.0" prefWidth="92.0">
            <styleClass>
               <String fx:value="btn-secondary" />
               <String fx:value="btn" />
            </styleClass>
            <graphic>
               <ImageView fitHeight="64.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../../img/icon/back.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
         <Button fx:id="btnBack1" layoutX="113.0" layoutY="564.0" mnemonicParsing="false" onAction="#onBtnBackClick" prefHeight="72.0" prefWidth="237.0">
            <styleClass>
               <String fx:value="btn-secondary" />
               <String fx:value="btn" />
            </styleClass>
            <graphic>
               <ImageView fitHeight="64.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../../img/icon/arrow.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
        </children>
    </Pane>
</AnchorPane>
